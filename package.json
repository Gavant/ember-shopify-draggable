{
  "name": "@gavant/ember-shopify-draggable",
  "version": "0.3.3",
  "description": "Ember components for Shopify's drag and drop",
  "keywords": [
    "ember-addon",
    "ember",
    "drag",
    "drop",
    "dragdrop",
    "draggable",
    "shopify",
    "gavant"
  ],
  "homepage": "https://github.com/Gavant/ember-shopify-draggable#usage",
  "bugs": {
    "url": "https://github.com/Gavant/ember-shopify-draggable/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gavant/ember-shopify-draggable.git"
  },
  "license": "MIT",
  "author": "Gavant Software",
  "contributors": [
    "Adam Baker <abaker@gavant.com>",
    "Bill Dami <bdami@gavant.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@shopify/draggable": "1.0.0-beta.12",
    "ember-auto-import": "^1.2.20",
    "ember-cli-babel": "7.23.1",
    "ember-cli-htmlbars": "^5.3.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "auto-changelog": "^2.2.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.25.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.0.4",
    "ember-cli-github-pages": "^0.2.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^8.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.4.1",
    "sass": "^1.14.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://gavant.github.io/ember-shopify-draggable/"
  },
  "volta": {
    "node": "10.19.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
